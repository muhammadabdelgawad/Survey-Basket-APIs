{
    "SeriLog": {
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft.AspNetCore": "Warning",
                "Hangfire": "Information"
            }
        },
        "WriteTo": [
            {
                "Name": "Console"
            },
            {
                "Name": "File",
                "Args": {
                    "path": "F:\\Logs\\log-.txt",
                    "rollingInterval": "Day",
                    "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact"

                }
            }
        ],
        "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
        "Properities": {
            "ApplicationName": "SurveyBasket"
        }
    },
    "AllowedHosts": "*",
    "ConnectionStrings": {
        "DefaultConnection": "Server = MUHAMMAD\\SQLEXPRESS;Database=SurveyBasketDb;Trusted_Connection=True;TrustServerCertificate=True;",
        "HangfireConnection": "Server = MUHAMMAD\\SQLEXPRESS;Database=SurveyBasketJobs;Trusted_Connection=True;TrustServerCertificate=True;"
    },
    "Jwt": {
        "Key": "",
        "Issuer": "SurveyBasketApp",
        "Audience": "SurveyBasketApp users",
        "DurationInMinutes": 60
    },
    "MailSettings": {
        "Mail": "katharina9@ethereal.email",
        "DisplayName": "SurveyBasket Support",
        "Password": "",
        "Host": "smtp.ethereal.email",
        "Port": 587
    }


